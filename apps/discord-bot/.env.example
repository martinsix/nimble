# Discord Application Settings
# Get these from https://discord.com/developers/applications

# Application ID (found in General Information)
DISCORD_APP_ID=your_application_id_here

# Public Key (found in General Information - used for verifying interactions)
DISCORD_PUBLIC_KEY=your_public_key_here

# Bot Token (found in Bot section - used for registering commands)
DISCORD_BOT_TOKEN=your_bot_token_here

# Optional: Guild ID for testing (faster command updates)
# Leave empty for global commands
DISCORD_GUILD_ID=

# Server Port (default 3002)
PORT=3002